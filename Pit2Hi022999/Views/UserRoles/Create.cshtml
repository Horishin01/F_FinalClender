@model IdentityUserRole<string>

@{
    if (!(Model is not null)) { throw new NullReferenceException(); }
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>UserRole</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label" asp-for="UserId"></label>
                <select class="form-select" asp-for="UserId" asp-items="ViewBag.UserId" autofocus></select>
                <p><span class="text-danger" asp-validation-for="UserId"></span>&nbsp;</p>
            </div>

             <div class="form-group">
                <label class="control-label" asp-for="RoleId"></label>
                <select class="form-select" asp-for="RoleId" asp-items="ViewBag.RoleId"></select>
                <p><span class="text-danger" asp-validation-for="RoleId"></span>&nbsp;</p>
            </div>

            <partial name="_EditPartial.cshtml" />

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
